<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <input type="text" name="song[note_contents][0]">
  <input type="text" name="song[note_contents][1]">
  <input type="text" name="song[note_contents][2]">

  <br />
  <%= f.submit %>
<%end%>
