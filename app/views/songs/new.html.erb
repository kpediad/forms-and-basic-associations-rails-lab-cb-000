<h1>Create a Song</h1>

<%= form_for @song  do |f| %>
  <%= f.label :title %>
  <%= f.text_field :title %>
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name %>
  <%= f.label :genre_name %>
  <%= f.collection_select :genre_name, Genre.all, :name, :name %>
  <%= f.fields_for 'song[note_contents][]', note_contents do |notes_form| %>
    <%= notes_form.text_field %>
    <%= notes_form.text_field %>
  <%end%>
  <br />
  <%= f.submit %>
<%end%>
